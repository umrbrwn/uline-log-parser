{
  "name": "nodejs-logger-zbzigh",
  "version": "0.1.0",
  "description": "CLI app to parses the input log file into standard JSONL output file.",
  "main": "index.js",
  "scripts": {
    "build": "eslint . --ext .ts && tsc",
    "start": "ts-node ./src/index.ts",
    "start:dev": "ts-node-dev --respawn ./src/index.ts -i ./sample-app.log -o ./sample-output.json",
    "lint": "eslint . --ext .ts",
    "test": "jest ./tests"
  },
  "keywords": [
    "Log parser"
  ],
  "author": "umrbrwn",
  "license": "ISC",
  "dependencies": {
    "@types/split2": "^3.2.1",
    "commander": "^10.0.0",
    "split2": "^4.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.36.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}
